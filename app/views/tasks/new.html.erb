 <div class="m-3 text-center">
   <h1>Create a task</h1>

   <%= form_for @task, html: {class: ""} do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.label :details %>
    <%= f.text_field :details %>
    <%= f.submit "Submit", class: "btn btn-success" %>
  <% end %>

 </div>
